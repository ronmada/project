<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="catalog.ProductFormController">
   <children>
      <Label id="lblProductID" layoutX="14.0" layoutY="64.0" text="Product ID:" />
      <Label id="lblProductName" layoutX="14.0" layoutY="103.0" text="Product Name:" />
      <Label id="lblProductType" layoutX="14.0" layoutY="142.0" text="Product Type:" />
      <TextField id="txtProductID" fx:id="txtProductID" editable="false" layoutX="100.0" layoutY="61.0" prefHeight="25.0" prefWidth="138.0" />
      <TextField id="txtProductName" fx:id="txtProductName" layoutX="101.0" layoutY="100.0" prefHeight="25.0" prefWidth="138.0" />
      <Button id="btnUpdate" fx:id="btnUpdate" layoutX="38.0" layoutY="234.0" mnemonicParsing="false" onAction="#changeProductInfo" text="Update" />
      <Button id="btnClose" fx:id="btnClose" layoutX="156.0" layoutY="234.0" mnemonicParsing="false" onAction="#closeProductInfo" text="Close" />
      <TextField id="txtProductType" fx:id="txtProductType" editable="false" layoutX="100.0" layoutY="138.0" prefHeight="25.0" prefWidth="138.0" />
   </children>
</AnchorPane>
